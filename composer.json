{
    "name": "hamoi1/livewire-charts",
    "description": "Livewire Charts",
    "keywords": [
        "asantibanez",
        "livewire-charts",
        "arukompas",
        "hamoi1",
        "Muhammad Esmail",
        "Hamo"
    ],
    "homepage": "https://github.com/hamoi1/livewire-charts",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrés Santibáñez",
            "email": "santibanez.andres@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Arunas Skirius",
            "email": "arunas@arunas.dev",
            "role": "Developer"
        },
        {
            "name": "Muhammad Esmail",
            "email": "ihama9728@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^7.3|^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.0|^9.5.10|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Hamoi1\\LivewireCharts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hamoi1\\LivewireCharts\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hamoi1\\LivewireCharts\\LivewireChartsServiceProvider"
            ],
            "aliases": {
                "LivewireCharts": "Hamoi1\\LivewireCharts\\LivewireChartsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}